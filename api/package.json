{
  "name": "api",
  "version": "1.0.0",
  "description": "api",
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon -e graphql -w ./src --exec 'ts-node-dev --respawn --no-notify src/index.ts'",
    "\n# Production": "",
    "production": "ts-node src/index.ts",
    "\n# Database": "",
    "migrate": "knex migrate:latest --cwd ./database",
    "migrate-make": "knex migrate:make --cwd ./database",
    "seed": "knex seed:run --cwd ./database",
    "seed-make": "knex seed:make --cwd ./database",
    "\n# Testing": "",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint --project ."
  },
  "license": "ISC",
  "nodemonConfig": {
    "ext": "graphql",
    "watch": [
      "src/"
    ],
    "delay": "200"
  },
  "dependencies": {
    "apollo-server": "^2.9.16",
    "dotenv": "^8.2.0",
    "graphql": "^14.5.8",
    "graphql-import": "^0.7.1",
    "graphql-middleware": "^4.0.2",
    "graphql-yup-middleware": "0.0.1",
    "knex": "^0.20.8",
    "knex-stringcase": "^1.4.1",
    "pg": "^7.17.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.7.5",
    "winston": "^3.2.1",
    "yup": "^0.28.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.20.1"
  }
}
